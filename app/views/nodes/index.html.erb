<h1>Listing nodes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>User</th>
    <th>Views</th>
    <th>Voteup</th>
    <th>Votedown</th>
    <th>Root</th>
    <th>Category</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @nodes.each do |node| %>
  <tr>
    <td><%= node.title %></td>
    <td><%= node.content %></td>
    <td><%= node.user_id %></td>
    <td><%= node.views %></td>
    <td><%= node.voteUp %></td>
    <td><%= node.voteDown %></td>
    <td><%= node.root %></td>
    <td><%= node.category_id %></td>
    <td><%= node.type_id %></td>
    <td><%= link_to 'Show', node %></td>
    <td><%= link_to 'Edit', edit_node_path(node) %></td>
    <td><%= link_to 'Destroy', node, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Node', new_node_path %>
